{
    "log": {
        "level": "trace", 
        "sinks": [
            {
                "sink": {
                    "level": "trace", 
                    "type": "stdout_sink_mt"
                }
            }, 
            {
                "sink": {
                    "file_name": "zebra.log", 
                    "max_file_size": 5000000, 
                    "max_files": 10, 
                    "type": "rotating_file_sink_mt"
                }
            }
        ]
    }, 
    "slash": {
        "instrus_filter": "", 
        "speed_file": "slash_speed.data"
    }, 
    "spy": {
        "device": "eth0", 
        "filter": "", 
        "nonblock": 0, 
        "promisc": 0, 
        "snaplen": 65535, 
        "timeout": 0
    }, 
    "zebra": {
        "optimization": false, 
        "packet_type": 99, 
        "pcap_file": "mixed.pcap", 
        "queue_size": 4096
    }
}